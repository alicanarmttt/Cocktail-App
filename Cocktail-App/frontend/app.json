{
  "expo": {
    "name": "Cocktail Roulette",
    "slug": "cocktail-roulette",
    "scheme": "cocktailroulette",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A0E15"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alican.barmenroulette",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.SENIN-IOS-REVERSE-CLIENT-ID-BURAYA"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.alican.barmenroulette",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A0E15"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#4A0E15"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-web-browser",
      "@react-native-google-signin/google-signin"
    ],
    "owner": "alicanarmttt",
    "extra": {
      "eas": {
        "projectId": "50e121bd-f4d6-47c1-a6c3-994e93e969c9"
      }
    }
  }
}
